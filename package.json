  "main": "index.js",
  "scripts": {
    "start:all": "docker-compose -f ./server/docker-compose.yml up -d && npx pm2 start 'npm run start:client'",
    "start:server": "docker-compose -f ./server/docker-compose.yml up -d",
    "start:client": "env-cmd -f ./config/.client.env npm start --prefix ./client/app",
    "exit:all": "docker-compose -f ./server/docker-compose.yml down && npx pm2 delete all",
    "bootstrap": "cd client/app && npm ci && cd ../../server/app && npm ci"
  },
